/* Map
---------------------------------------------------------------*/

body
{
	background-color:#004d9c;
}

#text {
	color:#0075b5;
	text-align:center;
}

#map {
	position: relative;
	width: 1012px;
	height: 666px;
}
#transparent_map {
	position: absolute;
	z-index: 30;
	width: 1012px;
	height: 666px;
	border: none;
}
#map ul {
	list-style: none;
}
#map li {
	position: absolute;
	z-index: 20;
	text-indent: -1000em;
	display: none;
}
